<li id="<%= feed_item.id %>">
  <%= link_to image_tag(feed_item.image_url, alt: feed_item.title, height: "50px", class: "bookimage") %>
  <span class="user">
    <%= link_to feed_item.user.name, feed_item.user %>
  </span>
  <span class="title"><%= feed_item.title %></span>
  <span class="timestamp">
    Added <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
</li>
